{
    "layer": "top",
    "position": "top",
    "height": 36,
    "margin-left": 0,
    "margin-right": 0,
    // "spacing": 6,
    "gtk-layer-shell": true,
    "passtrough": false,
    "mod": "dock",
    "exclusive": true,

//         "tray",
        // "custom/language",
//         "custom/updates",
    "modules-left": ["wlr/workspaces"],
    "modules-center": ["clock", "hyprland/window"],
    "modules-right": ["battery", "custom/updates", "pulseaudio", "backlight", "network", "cpu", "memory", "temperature", "custom/power"],

//     // "cpu": {
//     //     "interval": 10,
//     //     "format": "Ôò¶ {}%",
//     //     "max-length": 10,
//     //     "on-click": ""
//     // },
//     // "memory": {
//     //     "interval": 30,
//     //     "format": "Ôò™ {}%",
//     //     "format-alt":"Ôë≤ {used:0.1f}G",
//     //     "max-length": 10
//     // },
//     "tray": {
//         "icon-size": 13,
//         "tooltip": false,
//         "spacing": 10
//     },
    "custom/updates": {
        "exec": "(paru -Qua) | wc -l",
        "interval": 7200,
        "format": "Ôõô {}",
        "on-click": "foot -e paru -Syu" 
    },
    "custom/weather" : {
        "tooltip" : true,
        "format" : "{}",
        "restart-interval" : 30000,
        "exec" : ""
    },
    "custom/language": {
        "exec": "cat /tmp/kb_layout",
        "interval": 30,
        "format": "Ô†ã {}",
        "tooltip": false
    },
    "wlr/workspaces": {
        "all-outputs": true,
        "on-scroll-up": "hyprctl dispatch workspace e+1",
        "on-scroll-down": "hyprctl dispatch workspace e-1",
        "on-click": "activate",
        "format": "{name}",
    	  "on-click-middle": "minimize",
    	  "on-click-right": "close"
    },
    "battery": {
        "states": {
            "warning": 30,
            "critical": 15
        },
        "format": "{icon} {capacity}%",
        "tooltip-format": "{timeTo}, {capacity}%",
        "format-charging": "ÔñÉ {capacity}",
        "format-plugged": "Ôá¶ ",
        "format-alt": "{time} {icon}",
        "format-icons": ["Ôñç", "Ôï∫", "ÔïΩ", "Ôïø", "Ôï∏"]
    },
   "clock": {
        "format": "{:%Y-%m-%d - %I:%M}",
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "on-click": "foot -e tty-clock"
    },
    "cpu": {
        "format": "{usage}% Ôãõ",
        "tooltip": false
    },
    "memory": {
        "format": "{}% ÔÉâ"
    },
    "temperature": {
        // "hwmon-path": "/sys/class/hwmon/hwmon2/temp1_input",
        "critical-threshold": 40,
        "format": "{temperatureC}¬∞C {icon}",
        "format-icons": ["Ôù´", "Ôãâ", "Ôù©"]
    },
    "backlight": {
        // "device": "acpi_video1",
        "format": "{percent}% {icon}",
        "format-icons": ["Óéç", "Óèì", "Óèë", "Óèè", "Óèé", "Óèç", "Óèä", "Óèà", "Óéõ" ,"ÔÜÖ"],
        "on-scroll-up": "light -A 1",
        "on-scroll-down": "light -U 1"
    },
   "network": {
        "interface": "wlan0", 
        "format-wifi": "{essid} ({signalStrength}%) Ôá´",
        "format-ethernet": "{ipaddr}/{cidr} Ôá¶",
        "tooltip-format": "{ifname} via {gwaddr} Ôá¶",
        "format-linked": "{ifname} (No IP) Ôá¶",
        "format-disconnected": "Disconnected ‚ö†",
        "format-alt": "{ifname}: {ipaddr}/{cidr}"
    },
    "pulseaudio": {
        // "scroll-step": 1, // %, can be a float
        "format": "{volume}% {icon} {format_source}",
        "format-bluetooth": "{volume}% {icon}Ôäî {format_source}",
        "format-bluetooth-muted": "Ôö© {icon}Ôäî {format_source}",
        "format-muted": "Ôö© {format_source}",
        "format-source": "{volume}% ÔÑ∞",
        "format-source-muted": "ÔÑ±",
        "ignored-sinks": ["Easy Effects Sink"],
        "format-icons": {
            "headphone": "ÔÄ•",
            "hands-free": "ÔÄ•",
            "headset": "ÔÄ•",
            "phone": "ÔÇï",
            "portable": "ÔÇï",
            "car": "ÔÜπ",
            "default": ["ÔÄ¶", "ÔÄß", "ÔÄ®"]
        },
        "on-click": "pavucontrol"
    },
    "custom/power": {
        "format": "ÔÄë",
        //"on-click": "exec /home/focus/.config/scripts/power",
        "tooltip": false
    },
    "custom/separator": {
        "format": "Ôõú" 
    }, 
    "custom/launcher": {
      "format": "Û∞çú",
      "on-click": "rofi -show drun"
    },
    "custom/bluetooth": {
        //"exec": "~/.config/waybar/bluetooth.sh",
        //"interval": 5,
        "format": "Ôäî",
        "on-click": "$HOME/.local/bin/rofi-bluetooth",
        "tooltip": "Configure Bluetooth"
    },



    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t -sw",
    "on-click-right": "swaync-client -d -sw",
    "escape": true
}

